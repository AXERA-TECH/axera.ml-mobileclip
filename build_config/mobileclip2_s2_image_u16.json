{
  "model_type": "ONNX",
  "npu_mode": "NPU3",
  "quant": {
    "input_configs": [
      {
        "tensor_name": "DEFAULT",
        "calibration_dataset": "../imagenet-calib.tar",
        "calibration_size": 32,
        "calibration_mean": [
          0, 0, 0  // NOTICE: mobileclip2-s0 和 mobileclip2-s2 使用v1的预处理，v2的预处理方式在pulsar2中量化误差比较大
        ],
        "calibration_std": [
          255, 255, 255  
        ]
      }
    ],
    "calibration_method": "MinMax",
    "precision_analysis": true,
    "precision_analysis_method": "EndToEnd",
    "transformer_opt_level": 1,
    "layer_configs": [
      {
        "start_tensor_names": ["DEFAULT"],
        "end_tensor_names": ["DEFAULT"],
        "data_type": "U16",
        "weight_data_type": "S16"
      }
    ],
    "conv_bias_data_type": "FP32",
    "precision_analysis_mode": "NPUBackend",
    // "disable_auto_refine_scale": true
  },
  "input_processors": [
    {
      "tensor_name": "DEFAULT",
      "tensor_format": "RGB",
      "tensor_layout": "NCHW",
      "src_layout": "NCHW",
      "src_dtype": "FP32",
      "mean": [0, 0, 0],
      "std": [1, 1, 1]
    }
  ],
  "compiler": {
    "check": 0
  }
}
